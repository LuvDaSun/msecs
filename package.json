{
  "name": "msecs",
  "version": "1.0.2",
  "packageManager": "npm@10.5.0",
  "sideEffects": false,
  "type": "module",
  "main": "./bundled/main.cjs",
  "module": "./bundles/main.js",
  "types": "./types/main.d.ts",
  "browser": "./bundles/browser.js",
  "exports": {
    ".": {
      "require": "./bundled/main.cjs",
      "import": "./bundled/main.js",
      "types": "./types/main.d.ts",
      "browser": "./bundled/browser.js"
    }
  },
  "files": [
    "./types/**",
    "./bundled/**"
  ],
  "scripts": {
    "prepack": "node ./scripts/build.js",
    "pretest": "tsc",
    "build": "node ./scripts/build.js",
    "clean": "node ./scripts/clean.js",
    "test": "node --test ./transpiled/**/*.test.js",
    "spelling": "cspell",
    "formatting": "prettier --check *"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/LuvDaSun/msecs"
  },
  "author": "LuvDaSun Software B.V.",
  "license": "ISC",
  "dependencies": {
    "@types/node": "^20.11.30"
  },
  "devDependencies": {
    "@tsconfig/node20": "^20.1.4",
    "rollup": "^4.13.1",
    "typescript": "^5.4.3",
    "cspell": "^8.6.1",
    "prettier": "^3.2.5"
  },
  "engines": {
    "node": ">=18"
  }
}
